#!ipxe
menu anyDeploy PXE menu ${next-server}
item anydeploy Anydeploy
item boothdd Boot HDD

choose --default anydeploy --timeout 5000 target && goto ${target}
#choose os && goto ${os}

:anydeploy
kernel http://${next-server}/vmlinuz initrd=initrd.img nfsroot=${next-server}:/nfs/any64 rw ip=dhcp net.ifnames=0
initrd http://${next-server}/initrd.img
boot

:boothdd
 sanboot --no-describe --drive 0x80

